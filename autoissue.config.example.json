{
  "project": {
    "repo": "owner/repo-name",
    "path": "/path/to/project",
    "baseBranch": "main"
  },
  "executor": {
    "maxParallel": 3,
    "timeoutMinutes": 30,
    "createPr": true,
    "prDraft": false
  },
  "agent": {
    "model": "sonnet",
    "maxBudgetUsd": 5.0,
    "yolo": true
  },
  "maxTotalBudgetUsd": 50.0,
  "dashboard": {
    "enabled": true,
    "port": 3030,
    "auth": {
      "enabled": true,
      "type": "both",
      "username": "admin",
      "password": "change-me-in-production",
      "token": "generate-a-secure-token-here"
    }
  },
  "planner": {
    "enabled": true,
    "model": "sonnet",
    "maxBudgetUsd": 2.0
  }
}
