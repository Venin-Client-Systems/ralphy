{
  "name": "@venin/autoissue",
  "version": "2.0.0",
  "description": "Turn your GitHub backlog into pull requests, overnight",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Venin-Client-Systems/autoissue.git"
  },
  "keywords": [
    "ai",
    "github",
    "automation",
    "parallel",
    "claude",
    "cli",
    "issues",
    "autoissue"
  ],
  "bin": {
    "autoissue": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "dev": "tsx index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@types/react": "^19.2.14",
    "chalk": "^5.6.2",
    "commander": "^13.1.0",
    "ink": "^6.8.0",
    "ink-spinner": "^5.0.0",
    "lru-cache": "^11.2.6",
    "nanoid": "^5.1.2",
    "react": "^19.2.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.19.11",
    "@vitest/ui": "^4.0.18",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20"
  }
}
